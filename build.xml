<project name="geoserver" default="build">
  <target name="build" description="builds client">
    <exec executable="npm" failonerror="true">
       <arg line="install"/>
    </exec>
  </target>

  <target name="clean">
    <exec executable="grunt" failonerror="true">
       <arg line="clean"/>
    </exec>
  </target>

</project>
